{
  "mcpServers": {
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "98aa8e4e-7583-414f-b879-cadb1a4583c4"
      }
    },
    "neo4j": {
      "command": "/opt/homebrew/bin/uvx",
      "args": [
        "mcp-neo4j-data-modeling@0.2.0",
        "--transport",
        "stdio"
      ]
    },
    "firecrawl-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@Krieg2065/firecrawl-mcp-server",
        "--key",
        "af1abe1d-64eb-443e-8457-f42e6f8ee527",
        "--profile",
        "explicit-snail-AI7rKy"
      ]
    },
    "@21st-dev/magic": {
      "command": "npx",
      "args": [
        "-y",
        "@21st-dev/magic@latest"
      ],
      "env": {
        "API_KEY": "7336facdebd50448820fcbc4b0539dd34256cf64ffecccbebf11000caa026d55"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "mcp-sequentialthinking-tools": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-sequentialthinking-tools"
      ]
    },
    "supabase-mcp-primary": {
      "transport": "stdio",
      "command": "npx",
      "args": [
        "-y", 
        "@smithery/cli@latest",
        "run",
        "@supabase-community/supabase-mcp",
        "--stdio"
      ],
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096",
        "UV_THREADPOOL_SIZE": "16"
      }
    },
    "supabase-mcp-backup": {
      "transport": "http",
      "url": "https://server.smithery.ai/@supabase-community/supabase-mcp/mcp",
      "settings": {
        "timeout": 300000,
        "keepAlive": true,
        "maxRetries": 5,
        "retryDelay": 2000,
        "connectionTimeout": 60000,
        "requestTimeout": 180000,
        "healthCheck": {
          "enabled": true,
          "interval": 30000,
          "endpoint": "/health"
        }
      }
    },
    "cloudflare-playwright-mcp": {
      "transport": "http",
      "url": "https://server.smithery.ai/@cloudflare/playwright-mcp/mcp",
      "settings": {
        "timeout": 120000,
        "keepAlive": true,
        "maxRetries": 3,
        "retryDelay": 1000
      }
    }
  }
}